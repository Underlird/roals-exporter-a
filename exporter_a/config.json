    "target_date": "2026-01-20",
    "exporter_domain": "climate_og",
    "entities": ["sensor.0xd4fe28fffee1466d_humidity"],
    "registry_path": null,
    "strict_registry": true
  },
  "schema": {
    "run_mode": "str",
    "log_level": "str",
    "data_root": "str",
    "timezone": "str",
    "target_date": "str?",
    "exporter_domain": "str?",
    "entities": ["str"],
    "registry_path": "str?",
    "strict_registry": "bool"
  }
}
