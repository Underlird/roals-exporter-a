{
  "name": "ROALS Exporter A",
  "slug": "exporter_a",
  "description": "Deterministischer Daily-JSON Exporter (Exporter A)",
  "version": "0.1.2",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "map": ["share:rw"],
  "options": {
    "data_root": "/share/nara_data",
    "timezone": "Asia/Manila",
    "log_level": "INFO",
    "run_mode": "skeleton"
  },
  "schema": {
    "data_root": "str",
    "timezone": "str",
    "log_level": "list(INFO|DEBUG|WARNING|ERROR)",
    "run_mode": "list(skeleton|oneshot_today)"
  }
}
