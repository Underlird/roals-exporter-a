{
  "name": "ROALS Exporter A",
  "version": "0.4.4-lks",
  "slug": "roals_exporter_a",
  "description": "Truth Layer Exporter (LKS Sampling)",
  "arch": ["amd64", "aarch64", "armv7", "armhf"],
  "startup": "once",
  "boot": "manual",
  "hassio_api": true,
  "homeassistant_api": true,
  "map": ["share:rw"],
  "options": {
    "run_mode": "oneshot_date",
    "log_level": "DEBUG",
    "data_root": "/share/nara_data",
